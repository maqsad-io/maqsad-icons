{
  "name": "@maqsad/icons",
  "version": "0.2.28-alpha",
  "description": "Maqsad Icon Library for React - System and Illustration icons",
  "author": {
    "name": "Ahmad Feroz",
    "organisation": "Maqsad"
  },
  "license": "MIT",
  "homepage": "https://icons.maqsad.io",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./system": {
      "import": "./dist/esm/system/index.js",
      "require": "./dist/cjs/system/index.js",
      "types": "./dist/types/system/index.d.ts"
    },
    "./illustrations": {
      "import": "./dist/esm/illustrations/index.js",
      "require": "./dist/cjs/illustrations/index.js",
      "types": "./dist/types/illustrations/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm build:icons && pnpm build:ts",
    "build:icons": "tsx scripts/build-icons.ts",
    "build:ts": "pnpm build:esm && pnpm build:cjs && pnpm build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rm -rf dist src/system/icons src/illustrations/icons",
    "prepublishOnly": "pnpm clean && pnpm build",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "icons",
    "react",
    "svg",
    "maqsad",
    "icon-library"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "svgo": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=18.0.0"
  }
}
